{
    "http_listen"  : ":5000",
    "mysql_source" : "USER:PASSWORD@tcp(exmaple.com:3306)/database?parseTime=true&loc=Asia%2FSeoul",
    "sentry_dsn"   : "https://user@example.com/25",

    "dir": {
        "public" : "public",
        "upload" : "uploads"
    },

    "expires" : {
        "upload" : "24h",
        "idle"   : "672h",
        "forced" : "696h"
    },

    "cleanup" : {
        "duration"    : "1h",
        "query_limit" : 100
    },

    "id_rule" : {
        "min" : 5,
        "max" : 32,
        "chars" : "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789",
        "conflict" : [
            "ilI2",
            "ji",
            "Ss5",
            "Zz2"
        ]
    }
}
